<main>
    <div class="card mb-5">
        <div class="nav">
            {{#with navigation}}
            {{#each links}}
            <div class="nav-item">
                <a href="https://github.com/" class="nav-link {{#if @last}}disabled{{/if}}">
                    {{text}}
                </a>
            </div>
            {{/each}}
            {{/with}}
        </div>
    </div>
    <h1>{{header}}</h1>
    <div class="row mt-4">
        {{#with purchase}}
        <div class="nav nav-tabs">
            {{#each tabs}}
            <div class="nav-item">
                <a href="#" class="nav-link {{#if @first}} active {{/if}}">{{this}}</a>
            </div>
            {{/each}}
        </div>
        {{#with actionBlock}}
        {{#with input}}
        <div class="col-4 mt-4">
            <input class="form-control" placeholder="{{placeholder}}">
        </div>
        {{/with}}
        {{#each buttons}}
        <div class="col-2 mt-4">
            <a href="https://www.youtube.com/"
                class="btn {{#if isSuccess}} btn-success {{else}} btn-outline-success {{/if}} {{#if isDanger}} btn-outline-danger {{/if}}">{{text}}</a>
        </div>
        {{/each}}
        {{/with}}
        <hr class="my-4">
        <div class="row">
            {{#each itemList}}
            <div class="col-3">
                <p>{{number}}</p>
                <div><b>{{status}}</b></div>
            </div>
            <div class="col-2">
                {{#with amount}}
                <div>{{title}}</div>
                <h5>{{value}}</h5>
                {{/with}}
            </div>
            <div class="col-5">
                {{#each images}}
                <img src="https://picsum.photos/110/100" class="m-1">
                {{/each}}
            </div>
            <div class="col-2 mt-3">
                {{#with button}}
                <a href="https://www.youtube.com/" class="btn {{#if isPrimary}} btn-primary {{/if}}">{{text}}</a>
                {{/with}}
            </div>
            <hr class="my-1">
            {{/each}}
        </div>
        {{/with}}
        <hr class="my-5">
        {{#with userInfo}}
        <h2 class="mb-5">{{title}}</h2>
        <div class="card">
            <div class="card-body">
                {{#with profileData}}
                <h2 class="card-title">{{title}}</h2>
                <div class="row">
                    <div class="col-12">
                        <div class="card mb-2">
                            <div class="card-body">
                                {{#with fullName}}
                                <div class="row">
                                    {{#with surname}}
                                    <div class="col-4 ms-1">
                                        <div>{{title}}</div>
                                        <div><b>{{value}}</b></div>
                                    </div>
                                    {{/with}}
                                    {{#with name}}
                                    <div class="col-4">
                                        <div>{{title}}</div>
                                        <div><b>{{value}}</b></div>
                                    </div>
                                    {{/with}}
                                    {{#with middleName}}
                                    <div class="col-3">
                                        <div>{{title}}</div>
                                        <div><b>{{value}}</b></div>
                                    </div>
                                    {{/with}}
                                </div>
                                {{/with}}
                            </div>
                        </div>
                        <div class="card mb-2">
                            <div class="card-body">
                                {{#with otherInfo}}
                                <div class="row">
                                    {{#with birthday}}
                                    <div class="col-4 ms-1">
                                        <div>{{title}}</div>
                                        <div><b>{{value}}</b></div>
                                    </div>
                                    {{/with}}
                                    {{#with sex}}
                                    <div class="col-4">
                                        <div>{{title}}</div>
                                        <div><b>{{value}}</b></div>
                                    </div>
                                    {{/with}}
                                    {{#with language}}
                                    <div class="col-3">
                                        <div>{{title}}</div>
                                        <div><b>{{value}}</b></div>
                                    </div>
                                    {{/with}}
                                </div>
                                {{/with}}
                            </div>
                        </div>
                    </div>
                </div>
                {{/with}}
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            {{#each buttons}}
                            <div class="col-4">
                                <a href="https://www.youtube.com/"
                                    class="btn {{#if isPrimary}} btn-primary {{else}} btn-outline-secondary{{/if}} {{#if isDanger}} btn-outline-danger {{/if}}">{{text}}</a>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="card mt-3">
            <div class="card-body">
                <div class="row">
                    {{#with recipients}}
                    <h2>{{title}}</h2>
                    {{#each users}}
                    <div class="col-6">
                        <div class="card mt-2">
                            <div class="card-body">
                                <div><b>{{name}}</b></div>
                                <div>{{phone}}</div>
                                <div class="mt-2">
                                    {{#with button}}
                                    <a href="https://www.youtube.com/" class="btn btn-primary">{{text}}</a>
                                    {{/with}}
                                </div>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                    {{/with}}
                </div>
            </div>
        </div>
        <div class="card mt-3">
            <div class="card-body">
                <div class="row">
                    {{#with contacts}}
                    <h2>{{title}}</h2>
                    {{#each contactData}}
                    <div class="col-4">
                        <div class="card mt-2">
                            <div class="card-body">
                                <div><b>{{title}}</b></div>
                                <div>{{value}}</div>
                                <div class="mt-2">
                                    {{#with button}}
                                    <a href="https://www.youtube.com/"
                                        class="btn {{#if isPrimary}} btn-primary {{else}} btn-outline-secondary {{/if}} {{#if isDanger}} btn-outline-danger {{/if}}">{{text}}</a>
                                    {{/with}}
                                </div>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                    {{/with}}
                </div>
            </div>
        </div>
        {{/with}}
    </div>
    <hr class="mt-5 mb-3">
    <div class="nav">
        {{#each footer}}
        <div class="nav-item">
            {{#each this}}
            <a href="https://www.youtube.com/" class="nav-link m-2 {{#if @last}} disabled {{/if}}">{{text}}</a>
            {{/each}}
        </div>
        {{/each}}
    </div>
</main>