<main>
    <div class="card">
        <div class="card-body">
            {{#with heading}}
            <div class="card card-body">
                {{#with main}}
                <h1>{{title}}</h1>
                <p>
                    {{text}}
                </p>
                <div>
                    {{#with button_text}}
                    <div>
                        <button class="btn btn-primary">
                            {{solid}}
                        </button>
                    </div>
                    <br>
                    <div>
                        <button class="btn btn-outline-primary">
                            {{outline}}
                        </button>
                    </div>
                    {{/with}}
                </div>
                {{/with}}
            </div>
            <br>
            <div class="card card-body">
                {{#with second}}
                <h2>{{title}}</h2>
                <p>
                    {{text}}
                </p>
                <div>
                    {{#with button_text}}
                    <div>
                        <button class="btn btn-secondary">
                            {{solid}}
                        </button>
                    </div>
                    <br>
                    <div>
                        <button class="btn btn-outline-secondary">
                            {{solid}}
                        </button>
                    </div>
                    {{/with}}
                </div>
                {{/with}}
            </div>
            <br>
            <div class="card card-body">
                {{#with subblock}}
                <h3>{{title}}</h3>
                <p>
                    {{text}}
                </p>
                <div>
                    {{#with button_text}}
                    <div>
                        <button class="btn btn-dark">
                            {{solid}}
                        </button>
                    </div>
                    <br>
                    <div>
                        <button class="btn btn-outline-dark">
                            {{solid}}
                        </button>
                    </div>
                    {{/with}}
                </div>
                {{/with}}
            </div>
            {{/with}}
        </div>
    </div>
    <br>
    <br>
    <div class="card card-body">
        {{#with nested_display}}
        <div class="display-1">
            {{title}}
        </div>
        <div class="card card-body">
            {{#with children_display}}
            <div class="display-4">
                {{title}}
            </div>
            <p>
                {{text}}
            </p>
            <div>
                {{#with button}}
                <button class="btn btn-outline-warning">
                    {{text}}
                </button>
                {{/with}}
            </div>
            {{/with}}
        </div>
        <br>
        <div>
            <h6>{{header_text}}</h6>
        </div>
        <div>
            {{#with button}}
            <button class="btn btn-info">
                {{text}}
            </button>
            {{/with}}
        </div>
        {{/with}}
    </div>
    <br>
    <br>
    <div class="card card-body">
        {{#with paragraph}}
        <p class="text-uppercase">
            {{title}}
        </p>
        <p class="text-lowercase">
            {{text}}
        </p>
        <br>
        <div>
            {{#with button}}
            <button class="btn btn-link">
                <a href="https://google.com">
                    {{text}}
                </a>
            </button>
            {{/with}}
        </div>
        <div class="card card-body">
            <ul>
                {{#each list_name}}
                <li>
                    <div>
                        <button class="btn btn-link">
                            <a href="https://github.com/">
                                {{text}}
                            </a>
                        </button>
                    </div>
                </li>
                {{/each}}
            </ul>
        </div>
        {{/with}}
    </div>
</main>