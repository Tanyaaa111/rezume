<main>
    <div class="row">
        <div class="col-12 mb-3">
            <div class="card">
                <div class="nav">
                    {{#with navigation}}
                    {{#each links}}
                    <div class="nav-item">
                        <a href="https://github.com/" class="nav-link {{#if @last}}disabled{{/if}}">
                            {{text}}
                        </a>
                    </div>
                    {{/each}}
                    {{/with}}
                </div>
            </div>
        </div>
        <div class="col-12">
            <h1 class="my-5">{{header}}</h1>
        </div>
        <div class="row">
            <div class="col-8">
                {{#with contacts_data}}
                <h2 class="mb-3">{{title}}</h2>
                <div>
                    {{#each buttons}}
                    <button
                        class="btn {{#if isPrimary}} btn-warning {{else}} btn-outline-warning {{/if}}">{{text}}</button>
                    {{/each}}
                </div>
                <div class="mt-5">
                    {{#with form}}
                    <div class="row gy-3">
                        {{#with input}}
                        {{#with surname}}
                        <div class="col-6">
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                        </div>
                        {{/with}}
                        {{#with name}}
                        <div class="col-6">
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                        </div>
                        {{/with}}
                        {{#with phone}}
                        <div class="col-6">
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                        </div>
                        {{/with}}
                        {{#with email}}
                        <div class="col-6">
                            <label class="form-label">{{label}}</label>
                            <input class="form-control" placeholder="{{placeholder}}">
                            <div class="form-text">{{caption}}</div>
                        </div>
                        {{/with}}
                    </div>
                    <div class="my-5">
                        <hr>
                    </div>
                    {{/with}}
                    {{#each switch}}
                    <div class="form-check form-switch">
                        <label class="form-check-label">{{text}}</label>
                        <input type="checkbox" class="form-check-input">
                    </div>
                    {{/each}}
                    {{/with}}
                </div>
                {{/with}}
                <hr class="my-5">
                {{#with delivery}}
                <h2>{{title}}</h2>
                <div class="card">
                    <div class="card-body">
                        <div class="row">
                            {{#each select}}
                            <div class="col-6">
                                <label class="form-label">
                                    {{label}}
                                </label>
                                <select class="form-select" placeholder="{{placeholder}}">
                                    {{#each options}}
                                    <option value="{{value}}">{{text}}</option>
                                    {{/each}}
                                </select>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
                <div class="mt-3">
                    {{#each delivery_type}}
                    <div class="form-check">
                        <label class="form-check-label">{{type}}</label>
                        <input type="radio" class="form-check-input">
                    </div>
                    {{/each}}
                </div>
                <hr class="my-5">
                {{#with payment}}
                <h2 class="mb-3">{{title}}</h2>
                {{#each list}}
                <div class="form-check">
                    <label class="form-check-label">{{type}}</label>
                    <input class="form-check-input" type="radio">
                    <div class="form-text">{{description}}</div>
                </div>
                {{/each}}
                {{/with}}
                <hr class="my-5">
                <div class="row g-3">
                    {{#with contact}}
                    <h2 class="mb-3">{{title}}</h2>
                    {{#with surname}}
                    <div class="col-6">
                        <label class="form-label">{{label}}</label>
                        <input class="form-control" placeholder="{{placeholder}}">
                        <div class="form-text">{{caption}}</div>
                    </div>
                    {{/with}}
                    {{#with name}}
                    <div class="col-6">
                        <label class="form-label">{{label}}</label>
                        <input class="form-control" placeholder="{{placeholder}}">
                        <div class="form-text">{{caption}}</div>
                    </div>
                    {{/with}}
                    {{#with phone}}
                    <div class="col-6">
                        <label class="form-label">{{label}}</label>
                        <input class="form-control" placeholder="{{placeholder}}">
                        <div class="form-text">{{caption}}</div>
                    </div>
                    {{/with}}
                    {{#with email}}
                    <div class="col-6">
                        <label class="form-label">{{label}}</label>
                        <input class="form-control" placeholder="{{placeholder}}">
                        <div class="form-text">{{caption}}</div>
                    </div>
                    {{/with}}
                    {{/with}}
                </div>
                <hr class="my-5">
                <div>
                    {{#with coment}}
                    <label class="form-label">{{label}}</label>
                    <input class="form-control" placeholder="{{placeholder}}">
                    {{/with}}
                </div>
                {{/with}}
                <hr class="my-5">
                {{#with coupon}}
                <h2>{{title}}</h2>
                {{#with select}}
                {{#with availabel_coupon}}
                <div class="mb-3">
                    <label class="form-label">{{label}}</label>
                    <select class="form-select form-select-lg">
                        <option selected>{{placeholder}}</option>
                        {{#each options}}
                        <option value="{{value}}">{{text}}</option>
                        {{/each}}
                    </select>
                </div>
                {{/with}}
                {{#with friend_coupon}}
                <div class="mb-3">
                    <label class="form-label">{{label}}</label>
                    <select class="form-select form-select-sm">
                        <option selected>{{placeholder}}</option>
                        {{#each options}}
                        <option value="{{value}}">{{text}}</option>
                        {{/each}}
                    </select>
                </div>
                {{/with}}
                {{/with}}
                {{#with add}}
                {{#with friend}}
                <div class="mb-3">
                    <label class="form-label">{{label}}</label>
                    <input class="form-control form-control-lg" placeholder="{{placeholder}}">
                    <div class="form-text">{{caption}}</div>
                </div>
                {{/with}}
                {{#with colleague}}
                <div>
                    <label class="form-label">{{label}}</label>
                    <input class="form-control form-control-sm" placeholder="{{placeholder}}">
                    <div class="form-text">{{caption}}</div>
                </div>
                {{/with}}
                {{/with}}
                {{/with}}
                <hr class="my-5">
                {{#with quiz}}
                <h2>{{title}}</h2>
                {{#with preference}}
                <div class="form-label">{{title}}</div>
                {{#each options}}
                <div class="form-check">
                    <label class="form-label">{{this}}</label>
                    <input type="radio" class="form-check-input">
                </div>
                {{/each}}
                {{/with}}
                {{#with your_expirience}}
                <div class="mt-3">
                    <label class="form-label">{{label}}</label>
                    <input class="form-control">
                </div>
                {{/with}}
                {{#with purchase_frequency}}
                <div>
                    <label class="form-label"></label>
                    <select class="form-select">
                        <option selected>{{placeholder}}</option>
                        {{#each options}}
                        <option value="{{value}}">{{text}}</option>
                        {{/each}}
                    </select>
                </div>
                {{/with}}
                {{#with factors}}
                <div class="form-label mt-4">{{title}}</div>
                {{#each options}}
                <div class="form-check">
                    <label class="form-label">{{this}}</label>
                    <input type="checkbox" class="form-check-input">
                </div>
                {{/each}}
                {{/with}}
                {{/with}}
            </div>
            <div class="col-4">
                <div class="card mb-3">
                    <div class="card-body">
                        {{#each action_block}}
                        <div class="mb-3">
                            <button
                                class="btn {{#if isSuccess}} btn-success {{/if}} {{#if isDanger}} btn-danger {{/if}} {{#if isInfo}} btn-info {{/if}}">{{text}}</button>
                        </div>
                        {{/each}}
                    </div>
                </div>
                <div class="card mb-3">
                    {{#with total}}
                    <div class="card-title ps-3">
                        <h4 class="mt-3">{{title}}</h4>
                    </div>
                    <div class="card-body">
                        {{#each item_list}}
                        <div>{{name}} : {{price}}</div>
                        {{/each}}
                        <hr>
                        {{#each additional_info}}
                        <div>{{text}} : {{value}}</div>
                        {{/each}}
                        <hr>
                        {{#with total_price}}
                        <div>{{text}} : {{value}}</div>
                        {{/with}}
                        <hr>
                        {{#each button}}
                        <button
                            class="btn {{#if isPrimary}} btn-primary {{else}} btn-outline-primary {{/if}} mb-2">{{text}}</button>
                        {{/each}}
                    </div>
                    {{/with}}
                </div>
            </div>
        </div>
        <hr class="mt-5 mb-3">
        <div class="nav">
            {{#each footer}}
            <div class="nav-item">
                {{#each this}}
                <a href="'https://www.youtube.com/'" class="nav-link m-2 {{#if @last}} disabled {{/if}}">{{text}}</a>
                {{/each}}
            </div>
            {{/each}}
        </div>
</main>